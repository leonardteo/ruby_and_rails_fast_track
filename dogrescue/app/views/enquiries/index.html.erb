<h1>Listing enquiries</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Dog</th>
    <th>Body</th>
    <th>Phone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @enquiries.each do |enquiry| %>
  <tr>
    <td><%= enquiry.name %></td>
    <td><%= enquiry.email %></td>
    <td><%= enquiry.dog_id %></td>
    <td><%= enquiry.body %></td>
    <td><%= enquiry.phone %></td>
    <td><%= link_to 'Show', enquiry %></td>
    <td><%= link_to 'Edit', edit_enquiry_path(enquiry) %></td>
    <td><%= link_to 'Destroy', enquiry, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Enquiry', new_enquiry_path %>
